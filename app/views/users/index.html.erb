<%= render 'books/user', user: @user %>
<div class="col-md-1"></div>
<div class="col-md-8">
  <h2>Users</h2>
  <table class="table">
    <tr>
      <th>image</th>
      <th>Title</th>
      <th></th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user.id) %></td>
    </tr>
    <% end %>
  </table>
</div>